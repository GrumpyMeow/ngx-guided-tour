{
  "name": "ngx-guided-tour-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "ng build && npm run build:lib",
    "build:lib": "ng-packagr -p projects/ngx-guided-tour/ng-package.json && npm run build:lib:css && npm run build:lib:scss",
    "build:lib:css": "sass ./projects/ngx-guided-tour/src/lib/guided-tour-base-theme.scss ./dist/ngx-guided-tour/css/guided-tour-base-theme.css && sass ./projects/ngx-guided-tour/src/lib/guided-tour.component.scss ./dist/ngx-guided-tour/css/guided-tour.component.css",
    "build:lib:scss": "mkdir ./dist/ngx-guided-tour/scss/ && cp ./projects/ngx-guided-tour/src/lib/guided-tour-base-theme.scss ./dist/ngx-guided-tour/scss/guided-tour-base-theme.scss && cp ./projects/ngx-guided-tour/src/lib/guided-tour.component.scss ./dist/ngx-guided-tour/scss/guided-tour.component.scss",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish": "cp README.md dist/ngx-guided-tour/README.md && cd dist/ngx-guided-tour && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^6.6.7",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/language-service": "^17.0.0",
    "@types/jasmine": "2.8.16",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.42",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "jasmine-core": "4.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.17",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^17.0.0",
    "normalize-url": "^8.0.1",
    "protractor": "^7.0.0",
    "sass": "^1.77.8",
    "serialize-javascript": "^6.0.2",
    "trim-newlines": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
